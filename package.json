{
  "name": "tempoflex",
  "private": true,
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "build": "gulp",
    "package": "gulp package"
  },
  "devDependencies": {
    "@types/chrome": "0.0.86",
    "acorn": "^6.1.1",
    "browserify": "^16.2.3",
    "del": "^2.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.2.0",
    "gulp-zip": "^3.2.0",
    "humanize-duration": "^3.18.0",
    "merge-stream": "^2.0.0",
    "vinyl-source-stream": "^2.0.0",
    "workingday-uk": "^1.0.0"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true
    },
    "rules": {
      "eol-last": 0,
      "quotes": [
        2,
        "single"
      ]
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "modules": true
      }
    }
  }
}
